{
  "name": "ameeraz_management_system",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "db-stop": "npx supabase stop --backup",
    "execute": "node --no-warnings --experimental-specifier-resolution=node --loader ./loader.js",
    "script": "npm run execute scripts/setup.ts",
    "env": "npm run script scripts/test.ts",
    "confirm": "npm run script scripts/test.ts confirm",
    "db:gen": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "npm run script \"prisma db seed\" confirm",
    "db:studio": "npm run script \"prisma studio\"",
    "db:up": "docker compose up -d db",
    "db:down": "docker compose down db",
    "pretest:i": "npm run script \"npm run db:up && npx prisma migrate deploy\" test",
    "test:i": "npm run script \"vitest run --config ./vitest.config.integration.ts\" test",
    "posttest:i": "npm run db:down",
    "pretest:i:ui": "npm run script \"npm run db:up && npx prisma migrate deploy\" test",
    "test:i:ui": "npm run script \"vitest run --config ./vitest.config.integration.ts --ui --api 9527\" test",
    "posttest:i:ui": "npm run db:down"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@prisma/client": "^5.20.0",
    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "@remix-run/serve": "^2.12.1",
    "@table-library/react-table-library": "^4.1.7",
    "argon2": "^0.41.1",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "react-select": "^5.8.1",
    "remix-auth": "^4.1.0",
    "remix-auth-form": "^3.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@remix-run/dev": "^2.12.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.0",
    "@types/node": "^22.7.4",
    "@types/prompt": "^1.1.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.38",
    "prisma": "^5.20.0",
    "prompt": "^1.3.0",
    "remix-development-tools": "^4.7.1",
    "supabase": "^1.200.3",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
